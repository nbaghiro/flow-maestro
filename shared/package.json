{
    "name": "@flowmaestro/shared",
    "version": "1.0.0",
    "description": "Shared types and utilities for FlowMaestro",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "typescript": "^5.3.3"
    }
}
